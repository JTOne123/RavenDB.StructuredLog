{
  "Logging": {
    "IncludeScopes": true,
    "StructuredLogMaxOccurrences": 20, // This is the maximum number of occurrences that will be stored in a single StructuredLog. See StructuredLog.Occurrences for more info.
    "ExpirationInDays": 365, // How long before a structured log expires. Rolling value; logs that get updated will have their expire time reset to this value each update.
    "LogLevel": {
      "Default": "Debug",
      "System": "Critical",
      "Microsoft": "Critical"
    }
  }
}
